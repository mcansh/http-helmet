{
  "name": "@mcansh/remix-secure-headers",
  "version": "0.8.2",
  "description": "",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https:/github.com/mcansh/remix-security-headers",
    "directory": "./packages/http-helmet"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.cts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepare": "npm run copy-readme && npm run build",
    "copy-readme": "node copy-readme.js",
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@mcansh/http-helmet": "0.8.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
